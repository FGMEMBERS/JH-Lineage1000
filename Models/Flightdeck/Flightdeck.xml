	<!-- AP main switch -->
	<animation>
		<type>pick</type>
		<object-name>AP</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
                             <condition><equals>
                                <property>autopilot/settings/ap-armed</property>
                                <value>0</value>
                             </equals></condition>
		             <command>property-assign</command>
			     <property>controls/autoflight/yaw-damper[0]/engage</property>
                             <value>1</value>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>autopilot/settings/ap-armed</property>
			</binding>
		</action>

			</binding>
			<binding>
				<command>property-assign</command>
				<property>/devices/status/mice/mouse/mode</property>
                                <value>1</value>
			</binding>
		</action>
	</animation>

       <!-- YOKE UP DOWN AP SETTING -->
	<animation>
		<type>pick</type>
		<object-name>yoke.updown.left</object-name>
		<object-name>yoke.updown.right</object-name>
		<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value>vertical-speed-hold</value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/vertical-speed-fpm</property>
			     <step>100</step>  
			     <max>4500</max>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value>pitch-hold</value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-pitch-deg</property>
			     <step>0.1</step>
			     <max>9</max>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value>aoa-hold</value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-aoa-deg</property>
			     <step>1</step>
			     <max>45</max>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value>altitude-hold</value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-altitude-ft</property>
			     <step>100</step>
                             <min>1000</min>
			     <max>45000</max>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value>agl-hold</value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-agl-ft</property>
			     <step>100</step>
                             <min>1000</min>
			     <max>45000</max>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
                                <value></value>
                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-altitude-ft</property>
			     <step>100</step>
                             <min>1000</min>
			     <max>45000</max>
			</binding>
		</action>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>

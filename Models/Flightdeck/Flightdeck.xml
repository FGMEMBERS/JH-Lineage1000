	<!-- Autopilot Control Panel -->

        <!-- Nosewheel steering switch, in yoke -->
	<animation>
		<type>pick</type>
		<object-name>yoke.nsw.left</object-name>
		<object-name>yoke.nsw.right</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>/fdm/jsbsim/systems/NWS/engaged</property>
			</binding>
		</action>
	</animation>

	<animation>
		<type>select</type>
		<object-name>yoke.nsw.led.left</object-name>
		<object-name>yoke.nsw.led.right</object-name>
		<condition>
			<property>/fdm/jsbsim/systems/NWS/engaged</property>
		</condition>
	</animation>

        <!-- PTT -->
	<animation>
		<type>pick</type>
		<object-name>yoke.ptt.left</object-name>
		<object-name>yoke.ptt.right</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-assign</command>
				<property>/controls/radios/comm-ptt</property>
                                <value>1</value>
			</binding>
                        <mod-up><binding>
                 		<command>property-assign</command>
				<property>/controls/radios/comm-ptt</property>
                                <value>0</value>              
                        </binding></mod-up>
		</action>
	</animation>

	<animation>
		<type>select</type>
		<object-name>yoke.ptt.hot.left</object-name>
		<object-name>yoke.ptt.hot.right</object-name>
		<condition>
			<property>/controls/radios/comm-ptt</property>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>yoke.ptt.off.left</object-name>
		<object-name>yoke.ptt.off.right</object-name>
		<condition><equals>
			<property>/controls/radios/comm-ptt</property>
                        <value>0</value>
		</equals></condition>
	</animation>

        <!-- TCS -->
	<animation>
		<type>pick</type>

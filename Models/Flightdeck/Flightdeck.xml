	<!-- Autobrake Settings -->

	<animation>
		<type>pick</type>
		<object-name>AB.0</object-name>
		<object-name>AB.1</object-name>
		<object-name>AB.2</object-name>
		<object-name>AB.3</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>autopilot/autobrake/step</property>
				<value>0</value>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</binding>
		</action>
	</animation>

	<animation>
		<type>select</type>
		<object-name>AB.0</object-name>
		<condition>
				<equals>
					<property>autopilot/autobrake/step</property>
					<value>0</value>
				</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>AB.1</object-name>
		<condition>
				<equals>
					<property>autopilot/autobrake/step</property>
					<value>1</value>
				</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>AB.2</object-name>
		<condition>

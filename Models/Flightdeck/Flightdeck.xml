                             </equals></condition>
			     <command>property-adjust</command>
			     <property>autopilot/settings/target-pitch-deg</property>
			     <step>-0.1</step>
			     <min>-9</min>
			</binding>
			<binding>
                             <condition><equals>
                                <property>autopilot/locks/altitude</property>
